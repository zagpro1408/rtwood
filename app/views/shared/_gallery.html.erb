                <div>
                    <div class="gallery_image">
                        <% image = image_tag "product_1.jpg", class: "img-thumbnail img-responsive img-center center-block" %>
                        <a href="#modal_for_image_1" data-toggle="modal"><%= image %></a>
                    </div>
                    <!-- HTML-код модального окна-->
                    <div id="modal_for_image_1" class="modal fade">
                      <div class="modal-dialog">
                        <div class="modal-content">
                          <!-- Заголовок модального окна -->
                          <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                          </div>
                          <!-- Основное содержимое модального окна -->
                          <div class="modal-body">
                            <%= image_tag "product_1.jpg", class: "img-thumbnail img-responsive img-center center-block" %>
